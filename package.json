{
  "name": "ts-string-visitor",
  "version": "0.0.2",
  "description": "Generic TypeScript Visitor for String Enums and String Literal Union Types",
  "repository": {
    "type": "git",
    "url": "https://github.com/UselessPickles/ts-string-visitor.git"
  },
  "main": "dist/ts-string-visitor.js",
  "types": "dist/ts-string-visitor.d.ts",
  "scripts": {
    "compile": "tsc --project tsconfig.json",
    "test": "jest",
    "test:travis": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "author": "Jeff Lau",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "21.1.8",
    "coveralls": "3.0.0",
    "jest": "22.0.3",
    "ts-jest": "22.0.0",
    "typescript": "^2.6.2"
  },
  "keywords": [
    "typescript",
    "string",
    "enum",
    "literal",
    "visitor"
  ],
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "mapCoverage": true,
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
